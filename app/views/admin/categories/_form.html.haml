=simple_form_for [:admin, @category], html: { role: 'form' } do |f|
  .form-group
    =f.input :name, label: false, placeholder: 'Add name Category', input_html: { class: 'form-control' }
  .form-group
    =f.button :submit, class: 'btn btn-primary'
    =link_to "Back", admin_categories_path, class: 'btn btn-default'
