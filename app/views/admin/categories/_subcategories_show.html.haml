%table.table.table-striped
  %tbody
    -@category.subcategories.each do |subcategory|
      %tr
        %td=subcategory.name
        %td
          =link_to " ", [:edit, :admin, subcategory.category, subcategory], class: "glyphicon glyphicon-edit"
          =link_to " ", [:admin, subcategory.category, subcategory], method: :delete, class: "glyphicon glyphicon-trash"