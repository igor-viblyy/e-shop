!!!DOCTYPE html
%html
  %head
    %title EShop
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    = csrf_meta_tags

  %body
    .container-fluid
      .row
        %head
          =render 'admin/partials/menu'

      .row
        .col-md-12
          %ol.breadcrumb
            =render_breadcrumbs tag: :li, separator: " "
        = yield

      .row
        %footer

